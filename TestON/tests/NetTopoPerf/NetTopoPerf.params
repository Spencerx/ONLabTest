<PARAMS>
    <testcases>1,2</testcases>
    <tcpdump> 
        <intf>eth0</intf>
        <port>port 6633</port>
        <filename>~/packet_captures/Sanity.pcap</filename>
    </tcpdump>
    <CASE1>       
        <destination>h6</destination>
        <target>h40</target>
    </CASE1>       
    <PING>
        <source1>h7</source1>
        <target1>h32</target1>
        <source2>h8</source2>
        <target2>h33</target2>
    </PING>
    <LINK>
        <begin>s1</begin>
        <end>s2</end>
    </LINK>
    <YANK>
        <hostname>h1</hostname>
        <hostip>10.0.0.1</hostip>
        <hostmac>00:00:00:00:00:01</hostmac>
	<sw1>s1</sw1>
        <sw6>s6</sw6>
        <intf>s1-eth1</intf>
    </YANK>
    <PLUG>
        <intf>s1-eth1</intf>
	<sw6>s6</sw6>
	<sw1>s1</sw1>
    </PLUG>
    <CTRL>
        <ip0>10.128.5.50</ip0>
        <port0>6633</port0>
        <ip1>10.128.5.51</ip1>
        <port1>6633</port1>
        <ip2>10.128.5.52</ip2>
        <port2>6633</port2>
        <ip3>10.128.5.53</ip3>
        <port3>6633</port3>
        <ip4>10.128.5.54</ip4>
        <port4>6633</port4>
    </CTRL>
    <INTENTREST>
        <intentIP>10.128.5.52</intentIP>
        <intentPort>8080</intentPort>
        <intentURL>wm/onos/intent/high</intentURL>
    </INTENTREST>
    <RestIP0>10.128.5.50</RestIP0>
    <RestIP1>10.128.5.51</RestIP1>
    <RestIP2>10.128.5.52</RestIP2>
    <RestIP3>10.128.5.53</RestIP3>
    <RestIP4>10.128.5.54</RestIP4>
    <NR_Switches>25</NR_Switches>
    <NR_Links>50</NR_Links>
    <RESTCALL>
	<restIP1>10.128.5.51</restIP1>
	<restIP2>10.128.5.52</restIP2>
	<restPort>8080</restPort>
	<restURL>/wm/onos/topology/hosts</restURL>
    </RESTCALL>
</PARAMS>      

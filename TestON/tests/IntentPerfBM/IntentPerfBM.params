    <NR_Links>50</NR_Links>
    <RESTCALL>
	<restIP1>10.254.1.16</restIP1>
	<restIP2>10.254.1.17</restIP2>
	<restPort>8080</restPort>
	<restURL>/wm/onos/topology/hosts</restURL>
    </RESTCALL>

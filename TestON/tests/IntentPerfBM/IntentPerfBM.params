<PARAMS>
    <testcases>1,2,5</testcases>
    <tcpdump> 
        <intf>eth0</intf>
        <port>port 6633</port>
        <filename>~/packet_captures/Sanity.pcap</filename>
    </tcpdump>
    
    <INTENTS>
        <tableName>onos_perf</tableName>
        <urlAddIntent>http://10.254.1.17:8080/wm/onos/metrics?ids=Intents.AddOperation.BeginOperationTimestamp.EpochMs,Intents.AddOperation.EndOperationTimestamp.EpochMs</urlAddIntent>
        <urlRemIntent>http://10.254.1.17:8080/wm/onos/metrics?ids=Intents.RemoveOperation.BeginOperationTimestamp.EpochMs,Intents.RemoveOperation.EndOperationTimestamp.EpochMs</urlRemIntent>
        <urlAddIntentEnd>http://10.254.1.17:8080/wm/onos/metrics?ids=Intents.AddOperation.EndOperationTimestamp.EpochMs</urlAddIntentEnd>
        <url>http://10.254.1.17:8080/wm/onos/datagrid/add/intents/json</url>
        <url_new>http://10.254.1.17:8080/wm/onos/intent/high</url_new>
        <intent_id>1</intent_id>
        <intent_id2>11</intent_id2>
        <intent_type>shortest_intent_type</intent_type>
        <srcSwitch>00:00:00:00:00:00:30:00</srcSwitch>
        <dstSwitch>00:00:00:00:00:00:50:00</dstSwitch>
        <srcSwitch2>00:00:00:00:00:00:10:00</srcSwitch2>
        <dstSwitch2>00:00:00:00:00:00:70:00</dstSwitch2>
        <srcPort>1</srcPort>
        <dstPort>1</dstPort>
        <srcMac>00:00:00:00:00:03</srcMac> 
        <dstMac>00:00:00:00:00:05</dstMac>
        <srcMac2>00:00:00:00:00:01</srcMac2> 
        <dstMac2>00:00:00:00:00:07</dstMac2>
        <numFlows>1000</numFlows>
        <numIter>1</numIter>
        <hostIP>10.128.5.54</hostIP>
        <databaseScript>~/TestON/scripts/./perfdatagraph.py</databaseScript>
    </INTENTS>

    <CASE1>       
        <destination>h6</destination>
        <target>h40</target>
    </CASE1>       
    <PING>
        <source1>h7</source1>
        <target1>h32</target1>
        <source2>h8</source2>
        <target2>h33</target2>
    </PING>
    <LINK>
        <begin>s1</begin>
        <end>s2</end>
    </LINK>
    <YANK>
        <hostname>h1</hostname>
        <hostip>10.0.0.1</hostip>
        <hostmac>00:00:00:00:00:01</hostmac>
	<sw1>s1</sw1>
        <sw6>s6</sw6>
        <intf>s1-eth1</intf>
    </YANK>
    <PLUG>
        <intf>s1-eth1</intf>
	<sw6>s6</sw6>
	<sw1>s1</sw1>
    </PLUG>
    <CTRL>
        <ip1>10.254.1.16</ip1>
        <ip1_bm>192.168.200.16</ip1_bm>
        <port1>6633</port1>
        <ip2>10.254.1.17</ip2>
        <ip2_bm>192.168.200.17</ip2_bm>
        <port2>6633</port2>
        <ip3>10.254.1.24</ip3>
        <ip3_bm>192.168.200.24</ip3_bm>
        <port3>6633</port3>
    </CTRL>
    <INTENTREST>
        <intentIP>10.254.1.17</intentIP>
        <intentPort>8080</intentPort>

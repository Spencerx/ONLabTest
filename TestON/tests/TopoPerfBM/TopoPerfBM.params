<PARAMS>
    <testcases>1,6</testcases>
    <tcpdump> 
        <intf>eth0</intf>
        <port>port 6633</port>
        <filename>~/packet_captures/Sanity.pcap</filename>
    </tcpdump>
    
    <TOPO>
        <url_topo>/wm/onos/metrics?ids=Topology.EventNotification.LastEventTimestamp.EpochMs</url_topo>
        <url_links>http://10.254.1.16:8080/wm/onos/topology/links</url_links>
        <numIter>5</numIter>
        <hostIP>10.128.5.54</hostIP>
        <databaseScript>~/TestON/scripts/./perfdatagraph.py</databaseScript>
        <tableName>onos_perf</tableName>
        <switch1>00:00:00:00:00:00:10:00</switch1>
        <switch3>00:00:00:00:00:00:30:00</switch3>
    </TOPO>

    <CASE1>       
        <destination>h6</destination>
        <target>h40</target>
    </CASE1>       
    <PING>
        <source1>h7</source1>
        <target1>h32</target1>
        <source2>h8</source2>
        <target2>h33</target2>
    </PING>
    <LINK>
        <begin>s1</begin>
        <end>s2</end>
    </LINK>
    <YANK>
        <hostname>h1</hostname>
        <hostip>10.0.0.1</hostip>
        <hostmac>00:00:00:00:00:01</hostmac>
	<sw1>s1</sw1>
        <sw6>s6</sw6>
        <intf>s1-eth1</intf>
    </YANK>
    <PLUG>
        <intf>s1-eth1</intf>
	<sw6>s6</sw6>
	<sw1>s1</sw1>
    </PLUG>
    <CTRL>
        <ip1_bm>192.168.200.16</ip1_bm>
        <ip1>10.254.1.16</ip1>
        <port1>6633</port1>
        <ip2_bm>192.168.200.17</ip2_bm>
        <ip2>10.254.1.17</ip2>
        <port2>6633</port2>
        <ip3_bm>192.168.200.24</ip3_bm>
        <ip3>10.254.1.24</ip3>
        <port3>6633</port3>
    </CTRL>
    <INTENTREST>
        <intentIP>10.254.1.17</intentIP>
        <intentPort>8080</intentPort>
        <intentURL>wm/onos/intent/high</intentURL>
    </INTENTREST>
    <RestIP1>10.254.1.16</RestIP1>
    <RestIP2>10.254.1.17</RestIP2>
    <RestIP3>10.254.1.24</RestIP3>
    <NR_Switches>25</NR_Switches>
    <NR_Links>50</NR_Links>
    <RESTCALL>
	<restIP1>10.254.1.16</restIP1>
	<restIP2>10.254.1.17</restIP2>
	<restPort>8080</restPort>
	<restURL>/wm/onos/topology/hosts</restURL>
    </RESTCALL>
</PARAMS>      
